<swiper-page root-class="location">
  <view class="location__top-text">
    location
  </view>
  <view class="location__title">
    婚礼地点
  </view>
  <view class="location__map">
    <map
      id="map"
      class="map"
      subkey="43KBZ-XEWO3-MDS3P-YPLOZ-WGKSV-IFB3R"
      latitude="{{map.pos.latitude}}"
      longitude="{{map.pos.longitude}}"
      markers="{{map.markers}}"
      polygons="{{map.polygons}}"
      catch:touchmove="return false;"
    ></map>
  </view>
</swiper-page>
